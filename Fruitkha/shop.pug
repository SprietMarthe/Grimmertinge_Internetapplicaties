// PreLoader
.loader
    .loader-inner
        .circle
// PreLoader Ends
// header
#sticker.top-header-area
    .container
        .row
            .col-lg-12.col-sm-12.text-center
                .main-menu-wrap
                    // logo
                    .site-logo
                        a(href='index.html')
                            img(src='assets/img/logo.png' alt='')
                    // logo
                    // menu start
                    nav.main-menu
                        ul
                            li.current-list-item
                                a(href='#') Home
                                ul.sub-menu
                                    li
                                        a(href='index.html') Static Home
                                    li
                                        a(href='index_2.html') Slider Home
                            li
                                a(href='about.html') About
                            li
                                a(href='#') Pages
                                ul.sub-menu
                                    li
                                        a(href='404.html') 404 page
                                    li
                                        a(href='about.html') About
                                    li
                                        a(href='cart.html') Cart
                                    li
                                        a(href='checkout.html') Check Out
                                    li
                                        a(href='contact.html') Contact
                                    li
                                        a(href='news.html') News
                                    li
                                        a(href='shop.html') Shop
                            li
                                a(href='news.html') News
                                ul.sub-menu
                                    li
                                        a(href='news.html') News
                                    li
                                        a(href='single-news.html') Single News
                            li
                                a(href='contact.html') Contact
                            li
                                a(href='shop.html') Shop
                                ul.sub-menu
                                    li
                                        a(href='shop.html') Shop
                                    li
                                        a(href='checkout.html') Check Out
                                    li
                                        a(href='single-product.html') Single Product
                                    li
                                        a(href='cart.html') Cart
                            li
                                .header-icons
                                    a.shopping-cart(href='cart.html')
                                        i.fas.fa-shopping-cart
                                    a.mobile-hide.search-bar-icon(href='#')
                                        i.fas.fa-search
                    a.mobile-show.search-bar-icon(href='#')
                        i.fas.fa-search
                    .mobile-menu
                    // menu end
// end header
// search area
.search-area
    .container
        .row
            .col-lg-12
                span.close-btn
                    i.fas.fa-window-close
                .search-bar
                    .search-bar-tablecell
                        h3 Search For:
                        input(type='text' placeholder='Keywords')
                        button(type='submit')
                            | Search
                            i.fas.fa-search
// end search arewa
// breadcrumb-section
.breadcrumb-section.breadcrumb-bg
    .container
        .row
            .col-lg-8.offset-lg-2.text-center
                .breadcrumb-text
                    p Fresh and Organic
                    h1 Shop
// end breadcrumb section
