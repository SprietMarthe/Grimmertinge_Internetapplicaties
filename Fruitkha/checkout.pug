// search area
.search-area
    .container
        .row
            .col-lg-12
                span.close-btn
                    i.fas.fa-window-close
                .search-bar
                    .search-bar-tablecell
                        h3 Search For:
                        input(type='text' placeholder='Keywords')
                        button(type='submit')
                            | Search
                            i.fas.fa-search
// end search arewa
// breadcrumb-section
.breadcrumb-section.breadcrumb-bg
    .container
        .row
            .col-lg-8.offset-lg-2.text-center
                .breadcrumb-text
                    p Fresh and Organic
                    h1 Check Out Product
// end breadcrumb section
// check out section
.checkout-section.mt-150.mb-150
    .container
        .row
            .col-lg-8
                .checkout-accordion-wrap
                    #accordionExample.accordion
                        .card.single-accordion
                            #headingOne.card-header
                                h5.mb-0
                                    button.btn.btn-link(type='button' data-toggle='collapse' data-target='#collapseOne' aria-expanded='true' aria-controls='collapseOne')
                                        | Billing Address
                            #collapseOne.collapse.show(aria-labelledby='headingOne' data-parent='#accordionExample')
                                .card-body
                                    .billing-address-form
                                        form(action='index.html')
                                            p
                                                input(type='text' placeholder='Name')
                                            p
                                                input(type='email' placeholder='Email')
                                            p
                                                input(type='text' placeholder='Address')
                                            p
                                                input(type='tel' placeholder='Phone')
                                            p
                                                textarea#bill(name='bill' cols='30' rows='10' placeholder='Say Something')
                        .card.single-accordion
                            #headingTwo.card-header
                                h5.mb-0
                                    button.btn.btn-link.collapsed(type='button' data-toggle='collapse' data-target='#collapseTwo' aria-expanded='false' aria-controls='collapseTwo')
                                        | Shipping Address
                            #collapseTwo.collapse(aria-labelledby='headingTwo' data-parent='#accordionExample')
                                .card-body
                                    .shipping-address-form
                                        p Your shipping address form is here.
                        .card.single-accordion
                            #headingThree.card-header
                                h5.mb-0
                                    button.btn.btn-link.collapsed(type='button' data-toggle='collapse' data-target='#collapseThree' aria-expanded='false' aria-controls='collapseThree')
                                        | Card Details
                            #collapseThree.collapse(aria-labelledby='headingThree' data-parent='#accordionExample')
                                .card-body
                                    .card-details
                                        p Your card details goes here.
            .col-lg-4
                .order-details-wrap
                    table.order-details
                        thead
                            tr
                                th Your order Details
                                th Price
                        tbody.order-details-body
                            tr
                                td Product
                                td Total
                            tr
                                td Strawberry
                                td $85.00
                            tr
                                td Berry
                                td $70.00
                            tr
                                td Lemon
                                td $35.00
                        tbody.checkout-details
                            tr
                                td Subtotal
                                td $190
                            tr
                                td Shipping
                                td $50
                            tr
                                td Total
                                td $240
                    a.boxed-btn(href='#') Place Order
// end check out section
